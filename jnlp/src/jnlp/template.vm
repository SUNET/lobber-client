<?xml version="1.0" encoding="utf-8"?>
<jnlp
    spec="1.0+"
    codebase="%codebase%"
    href="launch.jnlp?code=%code%">
  <information>
    <title>Lobber Client</title>
    <vendor>NORDUnet<vendor/>
    <homepage>http://www.nordu.net<homepage/>
    <description kind="one-line">An applet for talking to a lobber service</description>
  </information>
  <security>
     <all-permissions/>
  </security>
  <resources>
    <j2se version="1.6+"
     initial-heap-size="32m"
     max-heap-size="512m"/>
     $dependencies
  </resources>
  <applet-desc main-class="$mainClass">
    <argument>%code%</argument>
  </applet-desc>
</jnlp>
